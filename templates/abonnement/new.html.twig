{% extends 'base.html.twig' %}

{% block title %}Nouvel Abonnement{% endblock %}

{% block body %}
    <h1>Nouvel Abonnement</h1>

    {{ form_start(form) }}
        {{ form_row(form.plan) }}
        {{ form_row(form.duree) }}
        {{ form_row(form.description) }}
        {{ form_row(form.prix) }}

        <button type="submit">Créer</button>
    {{ form_end(form) }}

    <a href="{{ path('abonnement_index') }}">Retour à la liste</a>
{% endblock %}
